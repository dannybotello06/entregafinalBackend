<div>
    <h1>realtime</h1>
        <h2>lista productos</h2>
        <form id="formNewProduct">
            <input type="text" name="title" placeholder="ingrese el nombre " required>
            <input type="text" name="description" placeholder="ingrese el descripcion " required>
            <input type="text" name="code" placeholder="ingrese el codigo " required>
            <input type="number" name="price" placeholder="ingrese el precio " required>
             <input type="text" name="category" placeholder="ingrese la categoria " required>

             <button type="submit">Enviar</button>
        </form>




    <ul id="productsList">
        {{#each products}}
        <li id={{this.id}}>{{this.title}}-{{this.price}}
        <button  id={{this.id}}>Borrar</button>
        </li>
        {{/each}}
    </ul>
   
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>